.box1{
  width: 100%;
  height: 650rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.box1 .content{
  height: 310rpx;
  width:246rpx;
  background-color: #fff;
}
.content .imgUrl{
  width:246rpx;
  height: 246rpx;
}
.content .name{
  padding: 10rpx 0;
  font-size: 10pt;
  color: #111; 
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;

}
.name text{
  color: #666;
  font-size: 8pt;
}