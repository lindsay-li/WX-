.new_l{
  width: 100%;
  height: 109rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
  border-bottom: 1rpx dotted #ccc;
}
.box{
    display: flex;
    width: 70%;
    height: 100%;
}
.box .indes{
    width: 15%;
    height: 109rpx;
    line-height: 109rpx;
    font-size: 12pt;
    color: #999;
}
.new_l .left{
  width: 85%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.left .s_name{
  font-size:12pt;
  color: #111; 
}
.left .s_air{
  width: 100%;
  font-size: 10pt;
  color: #9D9D9E;
}
.new_l .right{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.right image{
  width: 48rpx;
  height: 48rpx;
}